<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="search">

    <inherits name="com.google.gwt.user.User" />
    <inherits name="com.google.gwt.i18n.I18N"/>
    <inherits name="com.google.gwt.json.JSON" />
    <inherits name="com.google.gwt.http.HTTP" />
    <inherits name="com.google.common.collect.Collect" />

    <extend-property name="locale" values="en,de"/>
    <set-property-fallback name="locale" value="en"/>
    <set-property name="locale" value="en,de"/>
    <collapse-property name="locale" values="*" />

    <source path="client" />
    <entry-point class="com.alkacon.bootstrap.search.client.CmsSearchEntryPoint"></entry-point>

</module>