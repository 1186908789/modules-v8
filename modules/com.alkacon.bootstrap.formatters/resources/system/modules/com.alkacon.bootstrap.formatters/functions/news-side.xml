      <link type="WEAK">
        <target><![CDATA[/system/modules/com.alkacon.bootstrap.formatters/elements/news-side.jsp]]></target>
        <uuid>caabadf1-21db-11e3-b95f-000c29d28104</uuid>
      </link>
    </FunctionProvider>
    <Parameter>
      <Key><![CDATA[resourceType]]></Key>
      <Value><![CDATA[bs-newsarticle]]></Value>
    </Parameter>
    <Parameter>
      <Key><![CDATA[parentFolders]]></Key>
      <Value><![CDATA[("/shared/.content/newsarticles/" OR "/sites/default/")]]></Value>
    </Parameter>
    <Parameter>
      <Key><![CDATA[sortOrder]]></Key>
      <Value><![CDATA[desc]]></Value>
